		#container #quick .q_inner li:nth-child(6):hover a {background:url(../img/qu6.png) no-repeat 50% 35%;background-size:80px 80px;}
		#container #quick .q_inner li:nth-child(7):hover a {background:url(../img/qu7.png) no-repeat 50% 33%;background-size:80px 80px;}
		#container #quick .q_inner li:hover{background-color: #fff;font-weight:400;transition:all linear 0.6s}
