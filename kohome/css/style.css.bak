		#container #quick .q_inner li a span{text-align:center;position:absolute;left:33%;bottom:20px;font-weight:400}
		#container #quick .q_inner li:nth-child(n+6) a span{text-align:center;position:absolute;left:38%;bottom:20px;font-weight:400}

	/*cont1*/
		#container #cont1 {clear:both;}
		#container #cont1 .c1_h {position:relative;height:60px;width:100%;margin-top:50px;margin-bottom:20px}
		#container #cont1 .c1_h .c1_tit{position:absolute;left:10px;top:8px;font-family:'Noto sans KR';font-weight:400;font-size:40px}
		#container #cont1 .c1_h .c1_tit strong {font-weight:bold}
		#container #cont1 .c1_h .more1 {position:absolute;right:10px;top:15px;}
		#container #cont1 .c1_h .more1 img {width:28px;height:28px}

		#container #cont1 .news {padding-top:50px;width:98%;margin:0 auto}
		#container #cont1 .news > li {width:380px;height:325px;float:left;padding: 0 10px;position:relative}
		#container #cont1 .news > li a{display:block;overflow:hidden}
		#container #cont1 .news > li a:hover img{transform:scale(1.1);-ms-transition:all ease 0.5s;transition:all ease 0.5s}
		#container #cont1 .news > li a img {width:100%;height:100%}
		/*리스트 세개 더 추가하기*/
		#container #cont1 .news .txt_box {position:relative;color:#fff;width:380px;height:150px;background:rgba(0,0,0,0.6);position:absolute;bottom:0px}
		#container #cont1 .news .txt_box span{position:absolute;top:20px;left:15px;font-size:17px;font-weight:400}
		#container #cont1 .news .txt_box p{position:absolute;top:70px;left:15px;font-size:20px;font-weight:400}
	/*cont2*/
		#container #cont2 {clear:both;padding-top:50px}
		#container #cont2 > div{float:left;margin:0 10px}
		/*box1 탭소식*/
	    #container #cont2 .box1 {margin-left:0;width:48%;border:1px solid #ccc}
		#container #cont2 .box1 .tab{height:260px}
		#container #cont2 .box1 .tab ul{position:relative;border-bottom:1px solid #ccc;font-family:'Noto Sans KR';border-top:1px solid black}
		#container #cont2 .box1 .tab ul:after{content:"";display:block;clear:both}
		#container #cont2 .box1	.tab li{float:left;font-size:19px;font-weight:700;padding:0px 15px;height:60px;line-height:60px;color:#eee !important;}
