		#container #cont2 .box1 .tab > li > a {position:relative;display:block;text-decoration:none;
		border-bottom-color:#ccc;background:#f1f1f1;}
		#container #cont2 .box1 .tab > li > a > span{display:inline-block;letter-spacing:-1px;cursor:pointer;color:#eee !important;}
		#container #cont2 .box1 .tab li.active a{color:#000}
		#container #cont2 .box1 .tab li.active {background-color: #eee;}

		#container #cont2 .box1 .tab li ul{width:98%;position:absolute;top:70px;left:0;list-style:none;border:0;margin:0;padding:0;*zoom:1;transition:all linear 0.3s}
